{
    "name" : "h3ko group modifey BigBlueButton Screenshare Extension",
    "author": "h3ko group",
    "version" : "1.0",
    "manifest_version" : 2,
    "minimum_chrome_version": "34",
    "description" : "Chrome Extension granting WebRTC screen capturing capabilities to BigBlueButton meetings. Modifyed for h3ko group.",
    "homepage_url": "https://h3ko.de/",
    "background": {
        "scripts": ["background-script.js"],
        "persistent": true
    },
    "externally_connectable": {
       "matches": [
           "*://*.h3ko.de/*"
         ]
    },
    "icons" : {
        "128" : "icon.png"
    },
    "permissions": [
        "desktopCapture"
    ],
    "web_accessible_resources": [
        "icon.png"
    ]
}

